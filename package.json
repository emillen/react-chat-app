{
  "name": "react-chat-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "babel-node": "babel-node --presets=latest",
    "dev-server": "nodemon --ignore ./client --exec npm run babel-node --  ./bin/www",
    "dev-client": "cd client/ && npm start",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\""
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.0",
    "morgan": "~1.9.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5"
  }
}
